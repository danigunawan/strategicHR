<h1>All Users</h1>
<% if notice %>
  <p id="notice"><%= notice %> </p>
<%end %>

<table>
  <thead>
    <tr>
      <th>User Name</th>
      <th>Member Since</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user_path(user)  %></td>
        <td><%= user.created_at %></td>
     </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>

